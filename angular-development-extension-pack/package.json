{
    "name": "angular-development-extension-pack",
    "displayName": "Angular Development Extension Pack",
    "description": "This exension pack is a currated list of all the extension you may need for Angular development.",
    "version": "1.3.0",
    "scripts": {
        "vsce": "vsce",
        "vsce:package": "vsce package",
        "vsce:publish": "vsce publish",
        "vscode:publish": "npm run vsce:publish",
        "postversion": "git push && git push --tags && npm run vsce:package"
    },
    "engines": {
        "vscode": "^1.36.0"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/markusfalk/vscode-angular-extension-pack"
    },
    "publisher": "MarkusFalk",
    "icon": "logo-angular.png",
    "categories": [
        "Extension Packs"
    ],
    "extensionPack": [
        "angular.ng-template",
        "angulardoc.angulardoc-vscode",
        "cyrilletuzi.angular-schematics",
        "nrwl.angular-console",
        "raagh.angular-karma-test-explorer",
        "markusfalk.typescript-development-extension-pack"
    ]
}
